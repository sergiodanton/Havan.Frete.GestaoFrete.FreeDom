{
  "name": "havan-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "commit": "cz",
    "prepare": "husky install & yarn setup-mock",
    "setup-mock": "msw init public --save",
    "format": "prettier . --write --ignore-unknown",
    "format:check": "prettier . --check --ignore-unknown",
    "lint:style": "stylelint **/*.{css,scss} src/**/*.{js,jsx,ts,tsx} --allow-empty-input",
    "lint:style:fix": "yarn lint:style --fix",
    "lint:js": "eslint {src,tests,stories}/**/*.{ts,js,tsx,jsx,mjs,cjs} --no-error-on-unmatched-pattern --report-unused-disable-directives",
    "lint:js:fix": "yarn lint --fix",
    "lint": "yarn lint:js & yarn lint:style",
    "test": "jest --passWithNoTests --runInBand",
    "test:watch": "yarn test --watch",
    "test:staged": "yarn test --findRelatedTests",
    "test:coverage": "yarn test --coverage"
  },
  "dependencies": {
    "@freedom/react-components": "^1.0.1",
    "@freedom/tokens": "^1.0.0",
    "@reduxjs/toolkit": "^1.9.3",
    "axios": "^1.3.5",
    "change-case": "^4.1.2",
    "jose": "^4.14.1",
    "oidc-client-ts": "^2.2.2",
    "qs": "^6.11.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-oidc-context": "^2.2.2",
    "react-query": "^3.39.3",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.10.0",
    "redux": "^4.2.1",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@faker-js/faker": "^7.6.0",
    "@freedom/eslint-config-typescript": "^1.0.0",
    "@freedom/jest-config": "^1.0.0",
    "@freedom/prettier-config": "^1.0.0",
    "@freedom/stylelint-config": "^1.0.0",
    "@freedom/tailwind-config": "^1.0.0",
    "@originjs/vite-plugin-federation": "^1.2.2",
    "@testing-library/dom": "^9.0.1",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/qs": "^6.9.7",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-helmet": "^6.1.6",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.14",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.36.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.0",
    "msw": "^1.2.1",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "stylelint": "^15.2.0",
    "tailwindcss": "^3.3.1",
    "typescript": "^4.9.5",
    "vite": "^4.2.0"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
